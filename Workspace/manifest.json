{
    "manifest_version": 3,
    "name": "Taco Bell Nutrition Fixer",
    "version": "1.0",
    "description": "Fixes a specific CSS issue on Taco Bell's nutrition calculator page.",
    "permissions": [],
    "host_permissions": [
        "https://www.tacobell.com/nutrition/calculator"
    ],
    "icons": {
        "32": "icons/tacoBellGlitchIconSmall.png",
        "64": "icons/tacoBellGlitchIcon.png"
    },
    "content_scripts": [
        {
            "matches": [
                "https://www.tacobell.com/nutrition/calculator"
            ],
            "css": [
                "styles.css"
            ]
        }
    ]
}